<h2>Create a New User</h2>

<%= form_for @users do |f| %>
<% f.label :first_name %>
<% f.text_field :first_name %>
<% f.label :last_name %>
<% f.text_field :last_name %>
<% f.label :email %>
<% f.text_field :email %>
<% f.label :password_digest %>
<% f.text_field :password_digest %>
<% f.submit "Create User" %>
<%%>
